#include <stdio.h>
// std = Padrão
// io = Input/Output

// receber valor, se for positivo armazenar em a, se for negativo armazenar em b
int main() {
  int numero, a, b;

  printf("Digite um número: ");
  scanf("%d", &numero);
  
  if (numero > 0){ 
    a = numero;
    printf("O número é positivo");
  } else {
    b = numero;
    printf("o número é negativo");
  
  }  
  return 0;
}
