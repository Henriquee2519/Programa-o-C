// faça um algoritimo que peça dois numeros e imprima a soma

#include <stdio.h>
// std = Padrão
// io = Input/Output


int main() {
 int num1, num2, soma;

  printf("Informe o primeiro número: ");
  scanf("%d", &num1);

  printf("Informe o segundo número: ");
  scanf("%d", &num2);

  soma = num1 + num2;

  printf("A soma dos números é: %d\n", soma);

  return 0;
  }
