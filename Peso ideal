#include <stdio.h>
// std = Padrão
// io = Input/Output

// calcular o peso ideal de uma pessoa formula (72.7 * altura) - 58
int main() {
  float altura, peso_ideal;
  
 printf("Qual é sua altura? ");
 scanf("%f", &altura);

peso_ideal = (72.7 * altura) - 58;

printf("Seu peso ideal é %.2f\n", peso_ideal);

  return 0;

}
